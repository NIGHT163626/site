{
  "routes": [
    {
      "src": "/",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": ".*Roblox.*"
        }
      ],
      "headers": {
        "Content-Type": "text/plain; charset=utf-8"
      },
      "dest": "/script.lua"
    },
    {
      "src": "/",
      "dest": "/403.html",
      "status": 403
    }
  ]
}

{
  "routes": [
    {
      "src": "/",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": ".*Roblox.*"
        }
      ],
      "headers": {
        "Content-Type": "text/plain; charset=utf-8",
        "X-Content-Type-Options": "nosniff",
        "Cache-Control": "no-cache, no-store, must-revalidate"
      },
      "dest": "/api/roblox-script"
    },
    {
      "src": "/login.html",
      "headers": {
        "X-Content-Type-Options": "nosniff",
        "X-Frame-Options": "DENY",
        "X-XSS-Protection": "1; mode=block",
        "Referrer-Policy": "no-referrer"
      },
      "dest": "/login.html"
    },
    {
      "src": "/admin.html",
      "headers": {
        "X-Content-Type-Options": "nosniff",
        "X-Frame-Options": "DENY",
        "X-XSS-Protection": "1; mode=block",
        "Referrer-Policy": "no-referrer"
      },
      "dest": "/admin.html"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/.*",
      "headers": {
        "X-Content-Type-Options": "nosniff",
        "X-Frame-Options": "DENY",
        "X-XSS-Protection": "1; mode=block",
        "Referrer-Policy": "no-referrer"
      },
      "dest": "/403.html",
      "status": 403
    }
  ]
}
